{
  "name": "kevinquinnyo/advent-of-code",
  "description": "Advent Of Code 2018 Challenges",
  "type": "code-samples",
  "homepage": "https://github.com/kevinquinnyo/advent-of-code",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/kevinquinnyo/advent-of-code/issues"
  },
  "require": {
    "php": ">= 7.1",
    "squizlabs/php_codesniffer": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "kevinquinnyo\\AdventOfCode\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kevinquinnyo\\AdventOfCode\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always --configuration phpunit.xml.dist"
  },
  "config": {
    "sort-packages": true
  }
}
